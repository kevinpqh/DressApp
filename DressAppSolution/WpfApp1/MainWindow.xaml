                <Button x:Name="Dresses" Content="" HorizontalAlignment="Left" Height="66" Margin="24,360,0,0" VerticalAlignment="Top" Width="76" Cursor="Hand" Click="Dresses_Click"/>
                <Button x:Name="Glasses" Content="" HorizontalAlignment="Left" Margin="24,160,0,0" VerticalAlignment="Top" Width="73" Height="70" Cursor="Hand" IsHitTestVisible="False"/>
