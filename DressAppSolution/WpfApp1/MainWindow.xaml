                <Grid Margin="73,581,444,48">
                    <DockPanel Margin="34,-14,-34,14">
                        <DockPanel.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="Black" Offset="0"/>
                                <GradientStop Color="#FF295095" Offset="1"/>
                            </LinearGradientBrush>
                        </DockPanel.Background>
                        <DockPanel x:Name="Logo_1" Width="128" Margin="0,-25,0,2">
                            
                        </DockPanel>
                    </DockPanel>
                </Grid>
                <DockPanel Margin="369,575,52,60">
                    <DockPanel.Background>
                        <ImageBrush/>
                    </DockPanel.Background>
                    <DockPanel Margin="0,-32,0,0" Width="132">
                        <Grid Margin="0,-11,0,2" Width="156">
                            <DockPanel x:Name="Logo_2" Width="164" HorizontalAlignment="Right" Margin="-16,2,8,-1">
                                
                            </DockPanel>
                        </Grid>
                    </DockPanel>
                </DockPanel>
