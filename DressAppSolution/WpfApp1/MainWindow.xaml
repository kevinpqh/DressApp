        </DockPanel>
        <DockPanel x:Name="background_main" HorizontalAlignment="Left" LastChildFill="False" Margin="135,4,0,6" Width="636">
            
            <Grid Height="725" Margin="0,6,0,-2" VerticalAlignment="Top" Width="640">


                <Label Content="DressApp" HorizontalAlignment="Left" Height="76" Margin="229,48,0,0" VerticalAlignment="Top" Width="199" FontFamily="Pristina" FontSize="55" FontWeight="Bold"/>
                <Button x:Name="TextureEdit" Content="Editar Textura" HorizontalAlignment="Left" Height="70" Margin="106,422,0,0" VerticalAlignment="Top" Width="155" Click="EditTexture_Click" FontFamily="Pristina" FontSize="27" Background="#FFA2155B"/>
