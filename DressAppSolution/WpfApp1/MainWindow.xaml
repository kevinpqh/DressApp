            <Image Height="729" VerticalAlignment="Top" Width="100"/>
            <Image Height="729" VerticalAlignment="Top" Width="100"/>
            <Image Height="102" Margin="0,583,0,0" VerticalAlignment="Top" Width="134"/>
        </DockPanel>
        <DockPanel LastChildFill="False" Margin="780,3,6,1" Focusable="True" Cursor="Hand">
