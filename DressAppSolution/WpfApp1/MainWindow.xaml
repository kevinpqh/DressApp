        mc:Ignorable="d"
       
        Title="Administrador de ropas"
        Height="748" Width="1000" Background="#FF83394B" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Stretch" VerticalAlignment="Top" WindowStyle="ToolWindow" WindowStartupLocation="CenterScreen" AllowDrop="True">

